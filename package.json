{
  "name": "css-frameworks-ca",
  "version": "1.2.2",
  "description": "Test",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "build": "sass src/scss:dist/css",
    "start": "sass --watch src/scss:dist/css & live-server",
    "format": "prettier -w src/**/*.js",
    "lint": "eslint src/**/*.js",
    "lint-fix": "eslint src/**/*.js --cache --fix",
    "prepare": "husky install",
    "test:unit": "jest",
    "test:e2e": "cypress open",
    "test:e2e-cli": "cypress run",
    "test": "npm run test:unit && npm run test:e2e"
  },
  "keywords": [],
  "author": "Noroff",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.19.3",
    "@babel/preset-env": "^7.19.4",
    "@eslint/eslintrc": "^3.1.0",
    "@eslint/js": "^8.57.0",
    "cypress": "^13.11.0",
    "eslint": "^8.57.0",
    "eslint-plugin-cypress": "^3.3.0",
    "eslint-plugin-jest": "^28.6.0",
    "globals": "^15.4.0",
    "husky": "^9.0.11",
    "jest": "^29.2.0",
    "lint-staged": "^15.2.7",
    "live-server": "^1.2.2",
    "prettier": "^3.3.2",
    "sass": "^1.54.8"
  },
  "dependencies": {
    "bootstrap-dark-5": "^1.1.3",
    "dotenv": "^16.4.5"
  },
  "lint-staged": {
    "*.js": [
      "prettier --write",
      "eslint --fix"
    ],
    "*.html": [
      "prettier --write"
    ],
    "*.scss": [
      "prettier --write"
    ]
  }
}
